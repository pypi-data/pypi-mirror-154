{
  "_meta": {
    "description": "The index template for the shorten action of the URL shortener"
  },
  "composed_of": [
    "{prefix}-ecs-base",
    "{prefix}-ecs-ecs",
    "{prefix}-ecs-url"
  ],
  "index_patterns": [
    "{prefix}-shortener-urls"
  ],
  "template": {
    "settings": {
      "index": {
        "default_pipeline": "{prefix}-shortener-shorten"
      }
    }
  }
}
